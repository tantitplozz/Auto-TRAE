# ‚úÖ Trae AI Agent System - Environment Configuration
# Copy this file to .env and fill in your actual values

# üîê Security Configuration
JWT_SECRET=your-super-secret-jwt-key-here-make-it-long-and-random
ENCRYPTION_KEY=your-256-bit-encryption-key-here-32-characters-exactly
SALT_ROUNDS=12

# üìä Database Configuration
DATABASE_URL="postgresql://username:password@localhost:5432/trae_db"

# ü§ñ AI Model Configuration
# DeepSeek API
DEEPSEEK_API_KEY=your-deepseek-api-key-here
DEEPSEEK_MODEL=deepseek-chat

# Google Gemini API
GEMINI_API_KEY=your-gemini-api-key-here
GEMINI_MODEL=gemini-pro

# OpenRouter API
OPENROUTER_API_KEY=your-openrouter-api-key-here
OPENROUTER_MODEL=anthropic/claude-3-sonnet

# üîÑ Redis Configuration (for task queues)
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=your-redis-password-if-any

# üåê Server Configuration
PORT=3000
NODE_ENV=development
CORS_ORIGIN=http://localhost:5173

# üé≠ Proxy Configuration
PROXY_ENABLED=true
PROXY_ROTATION_INTERVAL=300000
DEFAULT_PROXY_PROVIDER=residential

# üîç Anti-Detection Configuration
STEALTH_MODE=advanced
FINGERPRINT_ROTATION=true
BEHAVIOR_SIMULATION=true
DETECTION_THRESHOLD=0.3

# üì± Browser Configuration
BROWSER_HEADLESS=false
BROWSER_TIMEOUT=30000
BROWSER_VIEWPORT_WIDTH=1920
BROWSER_VIEWPORT_HEIGHT=1080

# üöÄ Performance Configuration
MAX_CONCURRENT_AGENTS=5
TASK_TIMEOUT=300000
SESSION_WARMUP_DURATION=300

# üìù Logging Configuration
LOG_LEVEL=info
LOG_FILE=logs/trae.log
LOG_MAX_SIZE=10m
LOG_MAX_FILES=5

# üîî Webhook Configuration
WEBHOOK_URL=https://your-webhook-endpoint.com/notifications
WEBHOOK_SECRET=your-webhook-secret-key

# üí≥ Payment Configuration (if needed)
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=whsec_your_stripe_webhook_secret

# üìß Email Configuration (for notifications)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASS=your-app-password
EMAIL_FROM=noreply@trae-ai.com

# üîí Rate Limiting
RATE_LIMIT_WINDOW=900000
RATE_LIMIT_MAX_REQUESTS=100

# üéØ Feature Flags
ENABLE_AI_COMPETITION=true
ENABLE_STEALTH_NAVIGATION=true
ENABLE_PURCHASE_EXECUTION=true
ENABLE_SESSION_WARMING=true
ENABLE_FINGERPRINT_SPOOFING=true
ENABLE_BEHAVIOR_EMULATION=true

# üß™ Development Configuration
DEBUG_MODE=false
MOCK_AI_RESPONSES=false
SKIP_BROWSER_LAUNCH=false
ENABLE_DETAILED_LOGGING=true